{
  "compilerOptions": {
    "target": "esnext",
    "module": "nodenext",
    "jsx": "react-native",
    "allowJs": true,
    "esModuleInterop": true,
    "strict": true,
    "moduleResolution": "nodenext",
    "skipLibCheck": true
  },
  "exclude": [
    "babel.config.js",
    "react-native.config.js",
    "jest.config.js",
    "metro.config.js"
  ],
  "extends": "expo/tsconfig.base"
}
